{
  "services": [
    {
      "type": "web_service",
      "env": "python",
      "name": "THE13TH",
      "repo": "https://github.com/klohts/saas_demo",
      "branch": "main",
      "buildCommand": "pip install -r requirements.txt",
      "startCommand": "uvicorn main:app --host 0.0.0.0 --port $PORT",
      "envVars": [
        { "key": "DEMO_MODE", "value": "true" },
        { "key": "ADMIN_KEY", "value": "the13th-admin" }
      ]
    }
  ]
}
